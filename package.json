{
  "name": "@scrow/hooks",
  "version": "0.0.1",
  "description": "React hooks library for @scrow/core.",
  "main": "index.js",
  "author": "Christopher Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitescrow/hooks.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./use-signer": {
      "import": "./dist/use-signer/index.js",
      "types": "./dist/use-signer/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "vite --cors web",
    "load": "tsx --tsconfig test/tsconfig.json",
    "scratch": "yarn load test/scratch.ts"
  },
  "dependencies": {
    "@cmdcode/buff": "^2.2.5",
    "@cmdcode/signer": "^1.4.1",
    "@cmdcode/use-store": "^1.0.7",
    "@scrow/core": "^0.7.12",
    "dayjs": "^1.11.10",
    "plausible-tracker": "^0.3.8",
    "react-toastify": "^10.0.4",
    "sass": "^1.71.1",
    "swr": "^2.2.5"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@mantine/core": "^7.5.3",
    "@mantine/dates": "^7.5.3",
    "@mantine/form": "^7.5.3",
    "@mantine/hooks": "^7.5.3",
    "@tabler/icons-react": "^2.47.0",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
