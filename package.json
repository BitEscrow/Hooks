{
  "name": "@scrow/hooks",
  "version": "0.0.1",
  "description": "React hooks library for @scrow/core.",
  "main": "index.js",
  "author": "Christopher Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitescrow/hooks.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./use-signer": {
      "import": "./dist/use-signer/index.js",
      "types": "./dist/use-signer/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "vite --cors web",
    "load": "tsx --tsconfig test/tsconfig.json",
    "scratch": "yarn load test/scratch.ts"
  },
  "dependencies": {
    "@cmdcode/buff": "^2.2.4",
    "@cmdcode/signer": "^1.3.11",
    "@cmdcode/use-store": "^1.0.7",
    "@scrow/core": "^0.5.13",
    "dayjs": "^1.11.10",
    "swr": "^2.2.4"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@mantine/core": "^7.4.2",
    "@mantine/dates": "^7.4.2",
    "@mantine/form": "^7.4.2",
    "@mantine/hooks": "^7.4.2",
    "@tabler/icons-react": "^2.46.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
